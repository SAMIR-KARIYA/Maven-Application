pipeline {
    agent any

    stages {
        stage('Check Maven Version') {
            steps {
                script {
                    // Run the Maven version command using Windows batch script
                    def mvnVersion = bat(script: 'mvn --version', returnStdout: true).trim()

                    // Print the result
                    echo "Maven Version:\n${mvnVersion}"
                }
            }
        }
        stage('Compile Maven Project') {
            steps {
                script {
                    // Run the Maven compile command using Windows batch script
                    bat 'mvn compile'
                }
            }
        }
    }
}
